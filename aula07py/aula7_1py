# Lista
# Indicalização de uma variavel do tipo vazia
lista = []
# indicação de uma variavel lista, com elementos
lista = ["Marcela," 'Nicole','Matheus']
# lista de inteiros 
lista3 = [1,2,3,5]


lista = []
lista2 = ["Marcela," 'Nicole','Matheus']
lista3 = [1,2,3,5]

print(lista)
print(lista2)
print(lista3)

lista.append(lista2)
lista.append(lista3)

print(lista)

